package event;

/**
 *
 * @author Andy
 */
public class Point {

    private int x, z;

    public Point(int x, int z) {
        this.x = x;
        this.z = z;
    }

    public int getX() {
        return x;
    }

    public int getZ() {
        return z;
    }
}
